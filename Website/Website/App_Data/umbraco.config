        <flag>1522</flag>
        <Beer id="1739" key="5e685e01-7992-4dae-9551-00b86f131680" parentID="1540" level="4" creatorID="0" sortOrder="0" createDate="2016-03-23T23:18:15" updateDate="2016-03-23T23:18:15" nodeName="Fosters" urlName="fosters" path="-1,1083,1089,1540,1739" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
