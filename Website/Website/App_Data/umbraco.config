        </Beer>
        <Beer id="1603" key="c6b23df3-9f9b-4237-a998-93bda8e4d563" parentID="1545" level="4" creatorID="0" sortOrder="2" createDate="2016-02-29T18:16:33" updateDate="2016-02-29T18:16:33" nodeName="Cottage Delight Golden Premium Beer" urlName="cottage-delight-golden-premium-beer" path="-1,1083,1089,1545,1603" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
