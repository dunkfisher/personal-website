        <Beer id="1741" key="9b22c951-d468-4c7d-b83b-0de451f99bff" parentID="1545" level="4" creatorID="0" sortOrder="1" createDate="2016-03-23T23:21:32" updateDate="2016-03-23T23:45:18" nodeName="Beer 2014" urlName="beer-2014" path="-1,1083,1089,1545,1741" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
