        <Beer id="1701" key="7097d902-e28e-4eb1-9c93-07e7ca51f095" parentID="1545" level="4" creatorID="0" sortOrder="44" createDate="2016-03-03T20:59:09" updateDate="2016-09-24T00:28:58" nodeName="Ninkasi" urlName="ninkasi" path="-1,1083,1089,1545,1701" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
