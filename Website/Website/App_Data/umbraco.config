        <Beer id="1568" key="273e1ace-f635-40f4-b5b9-f2665263d10c" parentID="1112" level="4" creatorID="0" sortOrder="33" createDate="2016-02-04T00:15:47" updateDate="2016-09-05T22:43:22" nodeName="Gouden Carolus Ambrio" urlName="gouden-carolus-ambrio" path="-1,1083,1089,1112,1568" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
