        </Beer>
        <Beer id="1661" key="fc1401bf-be3a-4f45-9090-51e223f16e71" parentID="1107" level="4" creatorID="0" sortOrder="35" createDate="2016-03-02T20:55:10" updateDate="2016-03-02T20:55:10" nodeName="The West Highland Way" urlName="the-west-highland-way" path="-1,1083,1089,1107,1661" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
