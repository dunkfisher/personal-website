        <Beer id="2295" key="2abfda57-6112-4d0f-ae01-148d21ced746" parentID="1112" level="4" creatorID="0" sortOrder="9" createDate="2016-09-05T00:35:49" updateDate="2016-09-29T20:50:20" nodeName="Bersalis Kadet" urlName="bersalis-kadet" path="-1,1083,1089,1112,2295" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
