        <Beer id="2056" key="dc9153f6-d176-4738-bf9a-0d98bb5aa129" parentID="1112" level="4" creatorID="0" sortOrder="6" createDate="2016-09-05T00:18:30" updateDate="2016-09-22T18:51:30" nodeName="Arend Dubbel" urlName="arend-dubbel" path="-1,1083,1089,1112,2056" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
