        <Beer id="1108" key="5e135656-709d-4d67-b7a4-89da63cc54ea" parentID="1107" level="4" creatorID="0" sortOrder="0" createDate="2015-12-01T23:34:24" updateDate="2015-12-01T23:40:43" nodeName="The Cone" urlName="the-cone" path="-1,1083,1089,1107,1108" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
