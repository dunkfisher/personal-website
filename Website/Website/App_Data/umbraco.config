        </Beer>
        <Beer id="2101" key="ad6853d3-29b3-4151-9486-2434eecdd0fc" parentID="1112" level="4" creatorID="0" sortOrder="40" createDate="2016-09-05T00:21:48" updateDate="2016-09-22T19:09:09" nodeName="Hoegaarden Grand Cru" urlName="hoegaarden-grand-cru" path="-1,1083,1089,1112,2101" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
