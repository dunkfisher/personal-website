        <Beer id="1103" key="39e0638e-0a1f-4b33-be2d-5119d05b5f56" parentID="1100" level="4" creatorID="0" sortOrder="1" createDate="2015-12-01T23:07:47" updateDate="2015-12-01T23:17:13" nodeName="Apocalypse Now" urlName="apocalypse-now" path="-1,1083,1089,1100,1103" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
