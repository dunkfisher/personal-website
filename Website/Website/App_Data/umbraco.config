        <Beer id="1677" key="64bf9de6-ad00-4055-94a5-300c207f50f9" parentID="1107" level="4" creatorID="0" sortOrder="42" createDate="2016-03-03T19:46:43" updateDate="2016-03-03T19:46:43" nodeName="Birds &amp; Bees" urlName="birds-bees" path="-1,1083,1089,1107,1677" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
