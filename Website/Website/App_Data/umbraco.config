        <Beer id="1579" key="dd7b7977-6c8b-403c-b75b-5a0e84e5468d" parentID="1107" level="4" creatorID="0" sortOrder="0" createDate="2016-02-08T22:44:32" updateDate="2016-02-08T22:44:32" nodeName="Redact Red Ale" urlName="redact-red-ale" path="-1,1083,1089,1107,1579" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
