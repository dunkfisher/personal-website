        </Beer>
        <Beer id="1566" key="d02c8c0e-d88c-40a3-96bf-28eea4a14673" parentID="1112" level="4" creatorID="0" sortOrder="38" createDate="2016-02-04T00:03:14" updateDate="2016-09-05T22:43:23" nodeName="Grottenbier" urlName="grottenbier" path="-1,1083,1089,1112,1566" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
