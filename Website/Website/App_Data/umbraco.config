        </Beer>
        <Beer id="2109" key="9d9d8f7a-ccf1-4ef2-948c-280b09a96a64" parentID="1112" level="4" creatorID="0" sortOrder="37" createDate="2016-09-05T00:22:29" updateDate="2016-09-22T19:08:25" nodeName="Grimbergen Noel" urlName="grimbergen-noel" path="-1,1083,1089,1112,2109" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
