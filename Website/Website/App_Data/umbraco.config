        <Beer id="1571" key="7c129a2f-f2b6-46c5-a88f-4a262744f2e5" parentID="1112" level="4" creatorID="0" sortOrder="50" createDate="2016-02-06T21:00:06" updateDate="2016-09-24T00:26:34" nodeName="Kwak" urlName="kwak" path="-1,1083,1089,1112,1571" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
