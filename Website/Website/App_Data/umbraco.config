        <Beer id="1659" key="f20df933-ef03-45b1-94e2-f14864f2c8e3" parentID="1107" level="4" creatorID="0" sortOrder="34" createDate="2016-03-02T20:53:55" updateDate="2016-03-02T20:53:55" nodeName="Ola Dubh (12 Year Old)" urlName="ola-dubh-12-year-old" path="-1,1083,1089,1107,1659" isDoc="" nodeType="1093" creatorName="Duncan Fisher" writerName="Duncan Fisher" writerID="0" template="0" nodeTypeAlias="Beer">
